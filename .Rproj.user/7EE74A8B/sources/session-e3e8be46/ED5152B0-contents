                                DISPOSITION_INCIDENT_PATIENT_DISPOSITION_WITH_CODE_3_4_E_DISPOSITION_12_3_5_IT_DISPOSITION_112
                                ),
  evitals_12_col = VITALS_PULSE_OXIMETRY_E_VITALS_12,
